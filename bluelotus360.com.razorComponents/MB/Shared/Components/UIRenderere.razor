@using bluelotus360.com.razorComponents.MB.Shared.Components.Buttons

<MudGrid Class="@FromSection.CssClass">
    @if (FromSection.Children != null && FromSection.Children.Count > 0)
    {
        foreach (var uielement in FromSection.Children)
        {
            <span class="d-none element-key">@uielement.ElementKey</span>
            @if (uielement.ElementType != null && uielement.ElementType.Equals("OffSet"))
            {
                <MudItem xs="12" md="@uielement.Width" Class="@uielement.CssClass">
                </MudItem>

            }
            @if (uielement.ElementType != null && uielement.ElementType.Equals("Card"))
            {
                <BLCard DataObject="DataObject" InteractionLogics="InteractionLogics" UIElement="uielement" ObjectHelpers="ObjectHelpers" />
            }
            @if (uielement.ElementType != null && uielement.ElementType.Equals("ReadOnlyNB"))
            {
                <BLNumericBox UIElement="uielement" DataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" IsReadOnly=true />
            }
            @if (uielement.ElementType != null && uielement.ElementType.Equals("Label"))
            {

                <BLLabel UIElement="uielement" DataObject="DataObject" ObjectHelpers="ObjectHelpers"></BLLabel>
            }
            @if (uielement.ElementType != null && uielement.ElementType.Equals("MultiRadio"))
            {
                <BLMultiRadio UIElement="uielement" DataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers"></BLMultiRadio>
            }
            @if (uielement.ElementType != null && uielement.ElementType.Equals("CheckBox"))
            {

                <BLCheckBox UIElement="uielement" DataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" DynamicBindings="DynamicBindings"></BLCheckBox>
            }
            @if (uielement.ElementType != null && uielement.ElementType.Equals("Chip"))
            {
                <BLChip DataObject="DataObject" InteractionLogics="InteractionLogics" UIElement="uielement" ObjectHelpers="ObjectHelpers" />
            }
            @if (uielement.ElementType != null && uielement.ElementType.Equals("TimePicker"))
            {
                <BLTimePicker DataObject="DataObject" InteractionLogics="InteractionLogics" UIElement="uielement" ObjectHelpers="ObjectHelpers" />
            }
            @if (uielement.ElementID != null && uielement.ElementID.Equals("Typo"))
            {
                <BLTypo DataObject="DataObject" InteractionLogics="InteractionLogics" FromSection="uielement" ObjectHelpers="ObjectHelpers" />
            }
            @if (uielement.ElementType != null && uielement.ElementType.Equals("HeaderTitle"))
            {

                <BLHeaderTitle DataObject="DataObject" InteractionLogics="InteractionLogics" FromSection="uielement" ObjectHelpers="ObjectHelpers" />

            }
            @if (uielement.ElementType != null && uielement.ElementType.Equals("HorizontalLine"))
            {
                <MudItem md="uielement.Width" Class="@uielement.CssClass">
                    <MudDivider />
                </MudItem>
            }
            @if (uielement.ElementType != null && uielement.ElementType.Equals("ReadOnlyTextBox"))
            {
                <BLTextBox UIElement="uielement" DataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" IsReadOnly="true"></BLTextBox>

            }
            @if (uielement.ElementType != null && uielement.ElementType.Equals("ButtonGroup"))
            {
                <BLGroupButton DataObject="DataObject" InteractionLogics="InteractionLogics" FromSection="uielement" ObjectHelpers="ObjectHelpers" />
            }

            @if (uielement.ElementType != null && uielement.ElementType.Equals("SwitchGroup"))
            {
                <BLSwitchGroup DataObject="DataObject" InteractionLogics="InteractionLogics" FromSection="uielement" ObjectHelpers="ObjectHelpers" />
            }
            @if (uielement.ElementType != null && uielement.ElementType.Equals("TelDTimePicker"))
            {
                <BLDateAndTimePicker DataObject="DataObject" InteractionLogics="InteractionLogics" UIElement="uielement" ObjectHelpers="ObjectHelpers" />
            }

            @if (uielement.ElementType != null && uielement.ElementType.Equals("Tab"))
            {
               
                <BLTab DataObject="DataObject" InteractionLogics="InteractionLogics" FromSection="uielement" ObjectHelpers="ObjectHelpers" />
            }

            @if (FromSection.HasTelerikComponents)
            {
                @if (uielement.ElementType != null && uielement.ElementType.Equals("Cmb"))
                {
                    if (uielement.IsFreeze)
                    {
                        if (uielement.ElementID != null && uielement.ElementID.Equals("CodeBase"))
                        {
                            <BLCodeBaseMultiSelectCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers"/>
                        }
                    }
                    else
                    {
                        if (uielement.ElementID != null && uielement.ElementID.Equals("CodeBase"))
                        {
                            <BLCodeBaseCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" DynamicBindings="DynamicBindings" />
                        }
                        if (!string.IsNullOrEmpty(uielement.ElementID) && uielement.ElementID.Equals("Address"))
                        {
                            <BLAddressCombo UIElement="@uielement" ComboDataObject="@DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" />
                        }
                        if (uielement.ElementID != null && uielement.ElementID.Equals("Item"))
                        {
                            <BLItemCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" />
                        }
                        if (uielement.ElementID != null && uielement.ElementID.Equals("Unit"))
                        {
                            <BLUnitCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" />
                        }
                        if (uielement.ElementID != null && uielement.ElementID.Equals("Account"))
                        {
                            <BLAccountCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" />
                        }
                        if (uielement.ElementID != null && uielement.ElementID.Equals("Project"))
                        {
                            <BLProjectCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" />
                        }
                        if (uielement.ElementID != null && uielement.ElementID.Equals("Task"))
                        {
                            <BLTaskCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" />
                        }
                    }
                    
                }
                @if (uielement.ElementType != null && uielement.ElementType.Equals("NumericBox"))
                {

                    <BLNumericBox UIElement="uielement" DataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" />
                }
                @if (uielement.ElementType != null && uielement.ElementType.Equals("DatePicker") )
                {
                    <BLDatePicker DataObject="DataObject" InteractionLogics="InteractionLogics" UIElement="uielement" ObjectHelpers="ObjectHelpers" />
                }
                @if (uielement.ElementType != null && uielement.ElementType.Equals("Switch") )
                {
                    <BLSwitch FromSection="uielement" DataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers"></BLSwitch>
                }
                @if (uielement.ElementType != null && uielement.ElementType.Equals("Button"))
                {
                    <BLButton DataObject="DataObject" InteractionLogics="InteractionLogics" FromSection="uielement" ObjectHelpers="ObjectHelpers" />
                }
                @if (uielement.ElementType != null && uielement.ElementType.Equals("TextBox"))
                {
                    <BLTextBox UIElement="uielement" DataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers"></BLTextBox>
                }
                @if (uielement.ElementType != null && uielement.ElementType.Equals("TextArea"))
                {

                    <BLTextArea UIElement="uielement" DataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers"></BLTextArea>
                }


                @if (uielement.ElementType != null && uielement.ElementType.Equals("TelCmb"))
                {
                    if (uielement.ElementID != null && uielement.ElementID.Equals("CodeBase"))
                    {
                        //
                    }
                    if (uielement.ElementID != null && uielement.ElementID.Equals("Item"))
                    {
                        //
                    }
                    if (uielement.ElementID != null && uielement.ElementID.Equals("Unit"))
                    {
                        //
                    }
                    if (uielement.ElementID != null && uielement.ElementID.Equals("Account"))
                    {
                        
                        //
                    }
                }
                @if (uielement.ElementType != null && uielement.ElementType.Equals("TelTextBox"))
                {

                }
                @if (uielement.ElementType != null && uielement.ElementType.Equals("TelTextArea"))
                {
                    
                }
                @if (uielement.ElementType != null && uielement.ElementType.Equals("TelButton"))
                {
                    
                }
                @if (uielement.ElementType != null && uielement.ElementType.Equals("TelSwitch"))
                {

                }
                @if (uielement.ElementType != null && uielement.ElementType.Equals("TelDatePicker"))
                {

                }
                @if (uielement.ElementType != null && uielement.ElementType.Equals("TelNumericBox"))
                {

                }
                @if (uielement.ElementType != null && uielement.ElementType.Equals("TelDTimePicker"))
                {
                    //
                }
            }
            else
            {
                @if (uielement.ElementType != null && (uielement.ElementType.Equals("Cmb") || uielement.ElementType.Equals("TelCmb")))
                {
                    if (uielement.IsFreeze)
                    {
                        if (uielement.ElementID != null && uielement.ElementID.Equals("CodeBase"))
                        {
                            <BLCodeBaseMultiSelectCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers"/>
                        }
                        if (uielement.ElementID != null && uielement.ElementID.Equals("Address"))
                        {
                            <BLAddressSelectBox UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" />
                        }
                    }
                    else
                    {
                        if (uielement.ElementID != null && uielement.ElementID.Equals("CodeBase"))
                        {
                            <BLCodeBaseCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" DynamicBindings="DynamicBindings" />
                        }
                        @if (!string.IsNullOrEmpty(uielement.ElementID) && uielement.ElementID.Equals("Address"))
                        {
                            <BLAddressCombo UIElement="@uielement" ComboDataObject="@DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" DynamicBindings="DynamicBindings" />
                        }
                        if (uielement.ElementID != null && uielement.ElementID.Equals("Item"))
                        {
                            <BLItemCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" DynamicBindings="DynamicBindings" />
                        }
                        if (uielement.ElementID != null && uielement.ElementID.Equals("Unit"))
                        {
                            <BLUnitCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" />
                        }
                        if (uielement.ElementID != null && uielement.ElementID.Equals("Account"))
                        {
                            <BLAccountCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" DynamicBindings="DynamicBindings" />
                        }
                        if (uielement.ElementID != null && uielement.ElementID.Equals("Project"))
                        {
                            <BLProjectCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" DynamicBindings="DynamicBindings" />
                        }
                        if (uielement.ElementID != null && uielement.ElementID.Equals("Task"))
                        {
                            <BLTaskCombo UIElement="uielement" ComboDataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" DynamicBindings="DynamicBindings" />
                        }
                    }
                    
                }
                @if (uielement.ElementType != null && (uielement.ElementType.Equals("NumericBox") || uielement.ElementType.Equals("TelNumericBox")))
                {

                    <BLNumericBox UIElement="uielement" DataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" DynamicBindings="DynamicBindings"/>
                }
                @if (uielement.ElementType != null && (uielement.ElementType.Equals("DatePicker") || uielement.ElementType.Equals("TelDatePicker")))
                {
                    <BLDatePicker DataObject="DataObject" InteractionLogics="InteractionLogics" UIElement="uielement" ObjectHelpers="ObjectHelpers" DynamicBindings="DynamicBindings" />
                }
                @if (uielement.ElementType != null && (uielement.ElementType.Equals("Switch") || uielement.ElementType.Equals("TelSwitch")))
                {
                    <BLSwitch FromSection="uielement" DataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers"></BLSwitch>
                }
                @if (uielement.ElementType != null && (uielement.ElementType.Equals("Button") || uielement.ElementType.Equals("TelButton")))
                {
                    <BLButton DataObject="DataObject" InteractionLogics="InteractionLogics" FromSection="uielement" ObjectHelpers="ObjectHelpers" />
                }
                @if (uielement.ElementType != null && (uielement.ElementType.Equals("TextBox") || uielement.ElementType.Equals("TelTextBox")))
                {
                    <BLTextBox UIElement="uielement" DataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers" DynamicBindings="DynamicBindings"></BLTextBox>
                }
                @if (uielement.ElementType != null && (uielement.ElementType.Equals("TextArea") || uielement.ElementType.Equals("TelTextArea")))
                {
                    <BLTextArea UIElement="uielement" DataObject="DataObject" InteractionLogics="InteractionLogics" ObjectHelpers="ObjectHelpers"></BLTextArea>
                }
                
            }



            
        }
    }
</MudGrid>

